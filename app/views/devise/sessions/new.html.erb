<div id="frontPageWrapper">
	
<div id="frontpageLogoWrapper">
<a href="/"> <img id="frontpageLogo" src="http://files.bestpracticestore.com/files/images/bpslogowhiteclear.png"></a>
</div>

<p id="tagline">Existing Developer Sign In</p>

<div id="signInWrapper">
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>

  <div class="inputWrapper">
  	<%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true, class: "inputField" %>
  </div>

  <div class="inputWrapper">
  	<%= f.label :password %><br />
  <%= f.password_field :password, class: "inputField" %>
  </div>

  <% if devise_mapping.rememberable? -%>
   <div class="inputWrapper">
   	<%= f.check_box :remember_me %> <%= f.label :remember_me %>
   	</div>
  <% end -%>

  <div class="inputWrapper">
  	<%= f.submit "Sign in", class: "signInButton" %>
  	</div>
<% end %>

<!-- <%= render "devise/shared/links" %> -->

</div>
</div>
